{
    "name": "Pantheon Docs Search",
    "version": "0.0.1",
    "description": "Pantheon Docs Search will allow you to search through Pantheon's docs via Chrome Omnibox",
    "manifest_version": 2,
    "omnibox": {
      "keyword": "pd"
    },

    "background": {
      "persistent": false,
      "scripts": ["background.js"]
    },

    "permissions": [
      "tabs"
    ],
    "icons": {
      "16": "icons/icon_16.png",
      "32": "icons/icon_32.png",
      "48": "icons/icon_48.png"
    }
  }
